              "faraday-architecture/faq"
            ]
          },
          {
            "group": "Integration Guide",
            "pages": [
              "faraday-integration-guide/index",
              "faraday-integration-guide/setup",
              "faraday-integration-guide/getting-a-quote",
              "faraday-integration-guide/testing-a-solana-transaction",
              "faraday-integration-guide/testing-an-evm-transaction",
              "faraday-integration-guide/compliance-record-keeping"
            ]
          },
          {
            "group": "Product Info",
            "pages": [
              "faraday-product-info/supported-aggregators",
              "faraday-product-info/supported-chains",
              "faraday-product-info/supported-tokens"
            ]
          }
        ]
      },
      {
        "tab": "Range API",
        "groups": [
          {
            "group": "API reference",
            "openapi": "api-reference/range-api.json"
          },
          {
            "group": "Docs",
            "pages": [
              "api-reference/introduction",
              "data-api/data-introduction",
              "risk-api/risk-introduction",
